<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

        <TextView
            android:id="@+id/tl_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Hola"
            android:textSize="18sp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            />


        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/List_dogs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toStartOf="@+id/take_photo"
            android:backgroundTint="@color/color_primary"
            android:src="@drawable/baseline_list_alt"
            android:layout_margin="@dimen/activity_vertical_margin"
            android:contentDescription="@string/take_photo" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/take_photo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:backgroundTint="@color/color_primary"
            android:src="@drawable/baseline_add_a_photo"
            android:layout_margin="@dimen/activity_vertical_margin"
            android:contentDescription="@string/take_photo" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/take_photo"
            android:backgroundTint="@color/color_primary"
            android:src="@drawable/baseline_exit_to_app"
            android:layout_margin="@dimen/activity_vertical_margin"
            android:contentDescription="@string/take_photo" />


    </RelativeLayout>
</layout>